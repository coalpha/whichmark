{
   "manifest_version": 2,
   "name": "Whichmark",
   "version": "0.0.1",

   "description": "Switchmark compatable bookmark switcher",

   "applications": {
      "gecko": {
         "id": "whichmark@coalpha.github.io"
      }
   },

   "icons": {
      "48": "icons/48.png"
   },

   "permissions": [
      "bookmarks"
   ],

   "background": { "scripts": ["whichmark.js"] }
}
